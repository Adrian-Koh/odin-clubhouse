<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/styles.css" />
    <title>Excommunicado club</title>
  </head>
  <body>
    <%- include('navBar', links); %> <% if (locals.messages) { %>
    <h1>Excommunicado messages:</h1>
    <ul>
      <% for (const message of messages) { %> <% if (isMember) { %>
      <li><%= `${message.user} says: ${message.text}` %></li>
      <% } else { %>
      <li><%= message.text %></li>
      <% } %> <% } %>
    </ul>

    <% } %>
  </body>
</html>
